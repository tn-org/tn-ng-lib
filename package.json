{
  "name": "@tnlake/tn-ng-lib",
  "version": "1.0.4",
  "description": "Angular library for tnlake",
  "main": "dist/fesm2022/tn-ng-lib.mjs",
  "module": "dist/fesm2022/tn-ng-lib.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "tn-init-l10n": "src/scripts/init-l10n.js",
    "tn-build-l10n": "src/scripts/build-l10n.js",
    "tn-build-l10n-watch": "src/scripts/build-l10n-watch.js"
  },
  "files": [
    "dist",
    "src/scripts",
    "src/assets"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "prepare": "npm run build",
    "postinstall": "node src/scripts/postinstall.js",
    "build-l10n": "node src/scripts/build-l10n.js",
    "build-l10n:watch": "node src/scripts/build-l10n-watch.js"
  },
  "keywords": [
    "angular",
    "library",
    "l10n",
    "localization"
  ],
  "author": "tnlake",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tn-org/tn-ng-lib.git"
  },
  "bugs": {
    "url": "https://github.com/tn-org/tn-ng-lib/issues"
  },
  "homepage": "https://github.com/tn-org/tn-ng-lib#readme",
  "peerDependencies": {
    "@angular/core": "^20.0.0",
    "@angular/common": "^20.0.0"
  },
  "devDependencies": {
    "@angular/core": "^20.0.0",
    "@angular/common": "^20.0.0",
    "ng-packagr": "^20.0.0",
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "dotenv": "^16.0.0",
    "minimist": "^1.2.0",
    "js-yaml": "^4.1.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
