{
  "name": "@tnlake/tn-ng-lib",
  "version": "1.3.0",
  "description": "Angular library for tnlake",
  "main": "dist/fesm2022/tnlake-tn-ng-lib.mjs",
  "module": "dist/fesm2022/tnlake-tn-ng-lib.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "tn-init-l10n": "src/scripts/init-l10n.js",
    "tn-build-l10n": "src/scripts/build-l10n.js",
    "tn-build-l10n-watch": "src/scripts/build-l10n-watch.js"
  },
  "files": [
    "dist",
    "src/scripts",
    "src/assets"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "prepare": "npm run build",
    "build-l10n": "node src/scripts/build-l10n.js",
    "build-l10n:watch": "node src/scripts/build-l10n-watch.js"
  },
  "keywords": [
    "angular",
    "library",
    "l10n",
    "localization"
  ],
  "author": "tnlake",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tn-org/tn-ng-lib.git"
  },
  "bugs": {
    "url": "https://github.com/tn-org/tn-ng-lib/issues"
  },
  "homepage": "https://github.com/tn-org/tn-ng-lib#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/tnlake-tn-ng-lib.mjs"
    },
    "./l10n": {
      "types": "./dist/l10n/index.d.ts",
      "default": "./dist/l10n/fesm2022/tnlake-tn-ng-lib-l10n.mjs"
    },
    "./ion-core": {
      "types": "./dist/ion-core/index.d.ts",
      "default": "./dist/ion-core/fesm2022/tnlake-tn-ng-lib-ion-core.mjs"
    },
    "./ion-card": {
      "types": "./dist/ion-card/index.d.ts",
      "default": "./dist/ion-card/fesm2022/tnlake-tn-ng-lib-ion-card.mjs"
    },
    "./ion-fab": {
      "types": "./dist/ion-fab/index.d.ts",
      "default": "./dist/ion-fab/fesm2022/tnlake-tn-ng-lib-ion-fab.mjs"
    },
    "./ion-form": {
      "types": "./dist/ion-form/index.d.ts",
      "default": "./dist/ion-form/fesm2022/tnlake-tn-ng-lib-ion-form.mjs"
    },
    "./ion-grid": {
      "types": "./dist/ion-grid/index.d.ts",
      "default": "./dist/ion-grid/fesm2022/tnlake-tn-ng-lib-ion-grid.mjs"
    },
    "./ion-infinite-scroll": {
      "types": "./dist/ion-infinite-scroll/index.d.ts",
      "default": "./dist/ion-infinite-scroll/fesm2022/tnlake-tn-ng-lib-ion-infinite-scroll.mjs"
    },
    "./ion-list": {
      "types": "./dist/ion-list/index.d.ts",
      "default": "./dist/ion-list/fesm2022/tnlake-tn-ng-lib-ion-list.mjs"
    },
    "./ion-menu": {
      "types": "./dist/ion-menu/index.d.ts",
      "default": "./dist/ion-menu/fesm2022/tnlake-tn-ng-lib-ion-menu.mjs"
    },
    "./ion-overlay": {
      "types": "./dist/ion-overlay/index.d.ts",
      "default": "./dist/ion-overlay/fesm2022/tnlake-tn-ng-lib-ion-overlay.mjs"
    },
    "./ion-segment": {
      "types": "./dist/ion-segment/index.d.ts",
      "default": "./dist/ion-segment/fesm2022/tnlake-tn-ng-lib-ion-segment.mjs"
    },
    "./ion-tab": {
      "types": "./dist/ion-tab/index.d.ts",
      "default": "./dist/ion-tab/fesm2022/tnlake-tn-ng-lib-ion-tab.mjs"
    }
  },
  "peerDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0"
  },
  "devDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@ionic/angular": "^8.7.3",
    "@types/node": "^22.0.0",
    "dotenv": "^16.0.0",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.0",
    "ng-packagr": "^20.0.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
